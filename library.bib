mendeley-tags = {ESR},
number = {1},
pages = {74--84},
title = {{Electron spin relaxation time measurements using radiofrequency longitudinally detected ESR and application in oximetry}},
url = {internal-pdf:/Panagiotelis\_JMR\_V149\_2001.pdf},
volume = {149},
year = {2001}
}
