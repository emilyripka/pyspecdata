month = nov,
number = {11},
pages = {2017--24},
pmid = {15587923},
title = {{In vivo ESR studies on subcutaneously injected multilamellar liposomes in living mice.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15587923},
volume = {21},
year = {2004}
}
