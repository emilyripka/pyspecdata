pages = {11815--20},
pmid = {21730176},
title = {{Protein self-diffusion in crowded solutions.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3142006\&tool=pmcentrez\&rendertype=abstract},
