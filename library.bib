pmid = {20937902},
title = {{Crowding and hydrodynamic interactions likely dominate in vivo macromolecular motion.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2973006\&tool=pmcentrez\&rendertype=abstract},
