month = apr,
number = {14},
pages = {5032--7},
pmid = {15795383},
title = {{Defining the role of active-site loop fluctuations in dihydrofolate reductase catalysis.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=556001\&tool=pmcentrez\&rendertype=abstract},
