pmid = {15548022},
title = {{Local measures of intermolecular free energies in solution.}},
url = {internal-pdf:/Teng\_JACS\_126\_2004.pdf http://www.ncbi.nlm.nih.gov/pubmed/15548022},
volume = {126},
year = {2004}
