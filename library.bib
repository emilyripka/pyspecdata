month = jun,
number = {22},
pages = {5893--901},
pmid = {20454734},
publisher = {Royal Society of Chemistry},
title = {{Optimization of dynamic nuclear polarization experiments in aqueous solution at 15 MHz/9.7 GHz: a comparative study with DNP at 140 MHz/94 GHz.}},
