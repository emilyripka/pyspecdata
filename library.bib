journal = {Biophysical journal},
keywords = {Animals,CHO Cells,COS Cells,Cell Membrane,Cell Membrane: chemistry,Cercopithecus aethiops,Computer Simulation,Cricetinae,Cricetulus,Electron Spin Resonance Spectroscopy,Electron Spin Resonance Spectroscopy: methods,Membrane Lipids,Membrane Lipids: chemistry,Membrane Microdomains,Membrane Microdomains: chemistry,Mice,Models, Biological,Models, Molecular,Molecular Conformation,NIH 3T3 Cells,Phase Transition,Spin Labels,Temperature},
month = jun,
