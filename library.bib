file = {:C$\backslash$:/Users/franck/Documents/Mendeley Desktop/W\"{u}thrich, Otting, Liepinsh/W\"{u}thrich, Otting, Liepinsh - 1992 - Protein hydration in aqueous solution.pdf:pdf},
