number = {6},
pages = {1267--1278},
title = {{Nitric oxide adsorbed on zeolites: EPR studies}},
url = {http://www.sciencedirect.com/science/article/B6VNG-4BP9PPJ-K/2/8a9ce731e390807502c819e1cd45a245 },
volume = {60},
