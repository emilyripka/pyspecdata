pages = {45--56},
title = {{Adsorption and Desorption Behavior of NO on H-ZSM-5, Na-ZSM-5, and Na-A as Studied by EPR}},
url = {http://www.sciencedirect.com/science/article/B6WJX-45KNB16-5/2/ba6ccf588d2dcaafa178e0f480860994 },
volume = {155},
